<Models>
	<Model id='triage'>
		<Name>Ticket Triage</Name>
		<ArrivalRate>
			<Mon>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Mon>
			<Tue>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Tue>
			<Wed>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Wed>
			<Thu>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Thu>
			<Fri>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Fri>
			<Sat>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Sat>
			<Sun>
				<TimeBlock start="0" end="6">1</TimeBlock>
				<TimeBlock start="6" end="18">0</TimeBlock>
				<TimeBlock start="18" end="24">0</TimeBlock>
			</Sun>
		</ArrivalRate>
		<Activities>
			<Activity id="access">
				<DataInput>
					<DataObject id="ticket">
						<Fields>
							<Field name ="Value"/>
						</Fields>
					</DataObject>
				</DataInput>
				<DataOutput>
					<DataObject id="ticket">
						<Fields>
							<Field name="Class"/>
							<Field name ="Value"/>
						</Fields>
					</DataObject>
				</DataOutput>
			</Activity>
			<Activity id="request"/>
			<Activity id="register"/>
			<Activity id="schedule"/>
			<Activity id="compute"/>
			<Activity id="payment"/>
			<Activity id="receipt"/>
			<Activity id="chargeAccount"/>
		</Activities>
		<Gateways>
			<Gateway id="user">
				<Name>Is a machine free?</Name>
				<Type>Choice</Type>
				<Distribution>
					<Gate id='yes'>0.5</Gate>
					<Gate id='no'>0.5</Gate>
				</Distribution>	
			</Gateway>
			<Gateway id="free">
				<Name>Is a machine free?</Name>
				<Type>Choice</Type>
				<Distribution>
					<Gate id='yes'>0.9</Gate>
					<Gate id='no'>0.1</Gate>
				</Distribution>	
			</Gateway>
			<Gateway id="done">
				<Name>Is the user finished computing?</Name>
				<Type>Choice</Type>
				<Distribution>
					<Gate id='yes'>0.5</Gate>
					<Gate id='no'>0.5</Gate>
				</Distribution>			</Gateway>
			<Gateway id="recurring">
				<Name>Is this a recurring job?</Name>
				<Type>Choice</Type>
				<Distribution>
					<Gate id='yes'>0.4</Gate>
					<Gate id='no'>0.6</Gate>
				</Distribution>
			</Gateway>
		</Gateways>
		<Transitions>
			<Transition source="START" destination="request">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="request" destination="user">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="user" source_gate="yes" destination="login">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="user" source_gate="no" destination="register">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="login" destination="request">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="register" destination="request">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="request" destination="free">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="free" source_gate="yes" destination="compute">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="free" source_gate="no" destination="schedule">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="schedule" destination="END">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="compute" destination="done">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="done" source_gate="yes" destination="payment">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="done" source_gate="no" destination="compute">
				<Duration>
					<Distribution type="Const" value="100" />
				</Duration>
			</Transition>
			<Transition source="payment" destination="recurring">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="recurring" source_gate="yes" destination="receipt">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="recurring" source_gate="no" destination="chargeAccount">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="chargeAccount" destination="END">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
			<Transition source="receipt" destination="END">
				<Duration>
					<Distribution type="Const" value="10" />
				</Duration>
			</Transition>
		</Transitions>
		<Deadline>50000000</Deadline>
	</Model>
</Models>